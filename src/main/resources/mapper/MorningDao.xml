<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.alexmmd.dog.dao.MorningDao">


  <insert id="insert">
    insert into `daily-morning`(morning)
    values (#{morning})
  </insert>
  <select id="queryMorning" resultType="java.lang.String">
    SELECT morning FROM `daily-morning` ORDER BY id DESC LIMIT 1;
  </select>

</mapper>

